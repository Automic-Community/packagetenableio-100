<POST_SCRIPT mode="1" replacementmode="1" state="1">
<OSCRI><![CDATA[:SET &FLAG# = 0
:SET &ID# =''
:SET &UUID# = ''
:SET &HND# = PREP_PROCESS_REPORT("JOBS",, "REP")
:    PROCESS &HND#
:         SET &VALUE# = GET_PROCESS_LINE(&HND#)
:          IF &FLAG# EQ 0
:           SET &ID_CHECK# = STR_FIND(&VALUE#,'"id":')
:               IF &ID_CHECK# NE 0
:                 DEFINE &ID_STRINGS#, string, 3
:                 FILL &ID_STRINGS#[] = STR_SPLIT(&VALUE#,'"id":')
:                 SET &ID# = STR_TRIM(&ID_STRINGS#[2])
:               ELSE
:                   SET &UUID_CHECK# = STR_FIND(&VALUE#,'"uuid":')
:                   IF &UUID_CHECk# NE 0
:                     DEFINE &UUID_STRINGS#, string, 3
:                     FILL &UUID_STRINGS#[] = STR_SPLIT(&VALUE#,'"uuid":')
:                     SET &UUID# = STR_TRIM(&UUID_STRINGS#[2])
:                   ELSE
:                       SET &NAME_CHECK# = STR_FIND(&VALUE#, '"name":')
:                       IF &NAME_CHECK# NE 0
:                         DEFINE &NAME_STRINGS#, string, 3
:                         FILL &NAME_STRINGS#[] = STR_SPLIT(&VALUE#,'"name":')
:                         SET &NAME# = STR_TRIM(&NAME_STRINGS#[2])
:                         SET &PREP_OUTPUT# = &NAME#
:                         INC PCK.AUTOMIC_TENABLE_IO.PUB.INCLUDE.PREPARE_OUTPUT
:                         SET &NAME# = &PREP_OUTPUT#
:                         IF &NAME# EQ &UC4RB_TIO_INPUT_NAME#
:                             SET &FLAG# = 100   !indicate given name found sucessful
:                         ENDIF
:                       ENDIF
:                   ENDIF
:               ENDIF
:          ENDIF
:   ENDPROCESS
:   CLOSE_PROCESS &HND#

! if flag is 100 means name field found and appropriate value is save other wise fail action

:SET &PREP_OUTPUT# = &ID#
:INC PCK.AUTOMIC_TENABLE_IO.PUB.INCLUDE.PREPARE_OUTPUT
:SET &ID# = &PREP_OUTPUT#

:SET &PREP_OUTPUT# = &UUID#
:INC PCK.AUTOMIC_TENABLE_IO.PUB.INCLUDE.PREPARE_OUTPUT
:SET &UUID# = &PREP_OUTPUT#

:if &FLAG# EQ 0
:    MODIFY_STATE RETCODE = 50
:    SET &ID# = ''
:    SET &UUID# = ''
:    SET &NAME# = ''
:endif

: SET &UC4RB_SAVED_ID# = &ID#
: SET &UC4RB_SAVED_UUID# = &UUID#
:PUBLISH &UC4RB_SAVED_ID#,,"WORKFLOW"
:PUBLISH &UC4RB_SAVED_UUID#,,"WORKFLOW"
]]></OSCRI>
</POST_SCRIPT>
<DOCU_Docu state="1" type="text">
<DOC><![CDATA[= Action name =
PCK.AUTOMIC_TENABLE_IO.PUB.ACTION.GET_AGENT_GROUP

= General description =
This action fetch details of all agent group and publishes UUID of provided Agent Group Name.

= Inputs =
* Scanner ID *  :   Provide the scanner id for which fetch agent group 
* Name *        :   Provide a Name of the Agent Group to get its details. 


= Behavior =
* if Name doesn't exist, action will be fail.


=Return Value=
&UC4RB_SAVED_ID#    : Agent Group ID
&UC4RB_SAVED_UUID#  : Agent Group UUID

= Outputs =
* Return code is 0 in case of success.
* Return code is non-zero in case of failure.

= Rollback =
(none)
]]></DOC>
</DOCU_Docu>
